{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/trending", "destination": "/trending.html" },
    { "source": "/favorites", "destination": "/favorites.html" }
  ]
}
