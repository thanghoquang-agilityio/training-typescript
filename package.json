{
  "name": "training-typescript",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "start:server": "npx ts-node ./server/index.js",
    "start": "rm -rf dist && rm -rf .parcel-cache && parcel src/pages/*.html",
    "build": "rm -rf build && rm -rf .parcel-cache && parcel build ./src/pages/*.html --no-cache --no-source-maps --dist-dir build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "json-server": "^0.17.3",
    "parcel": "^2.9.3",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "buffer": "^5.5.0||^6.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.49.0",
    "events": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.0",
    "prettier": "^3.0.3",
    "stream-browserify": "^3.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "yarn prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "staticFiles": {
    "staticPath": "src/assets",
    "outputPath": "dist/assets"
  }
}
